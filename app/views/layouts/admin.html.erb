
<!DOCTYPE html>
<html lang="zh-cn">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="shortcut icon" href="../../docs-assets/ico/favicon.png">
    <title>99 Chima Zhu Shou</title>

    <!-- Bootstrap core CSS -->
    <!-- Custom styles for this template -->
    <link href="offcanvas.css" rel="stylesheet">
    <%= stylesheet_link_tag    "bootstrap_admin" %>
    <%= javascript_include_tag "bootstrap_admin" %>
    <%= csrf_meta_tags %>

    <!-- Just for debugging purposes. Don't actually copy this line! -->
    <!--[if lt IE 9]><script src="../../docs-assets/js/ie8-responsive-file-warning.js"></script><![endif]-->

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="http://cdn.bootcss.com/html5shiv/3.7.0/html5shiv.min.js"></script>
      <script src="http://cdn.bootcss.com/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->
  </head>

  <body>
    <div class="navbar navbar-fixed-top navbar-inverse" role="navigation">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="/admin">99 唯一尺码助手</a>
        </div>
        <div class="collapse navbar-collapse">
          <ul class="nav navbar-nav">
            <li class="active"><a href="/admin">Home</a></li>
            <li><%=link_to "问答助手", admin_questions_path %></li>
          </ul>
        </div><!-- /.nav-collapse -->
      </div><!-- /.container -->
    </div><!-- /.navbar -->

    <div class="container">

      <div class="row row-offcanvas row-offcanvas-right">

        <div class="col-xs-6 col-sm-2 sidebar-offcanvas" id="sidebar" role="navigation">
          <div class="list-group">
            <!-- <a href="#" class="list-group-item active">Link</a> -->
            <%= link_to "服装类型", admin_types_path,:class=> "list-group-item" %>
            <%= link_to "品牌" , admin_brands_path,:class=> "list-group-item" %>
            <%= link_to "型号" , admin_models_path,:class=> "list-group-item" %>
            <%= link_to "尺码" , admin_sizes_path,:class=> "list-group-item" %>
            <%= link_to "货号" , admin_items_path,:class=> "list-group-item" %>
            <%= link_to "品牌上衣尺寸" , admin_up_sizes_path,:class=> "list-group-item" %>
            <%= link_to "品牌下衣尺寸" , admin_down_sizes_path,:class=> "list-group-item" %>
            <%= link_to "身高" , admin_heights_path,:class=> "list-group-item" %>
            <%= link_to "体重" , admin_weights_path,:class=> "list-group-item" %>
            <%= link_to "预估尺寸(男)" , admin_forecast_msizes_path,:class=> "list-group-item" %>
            <%= link_to "预估尺寸(女)" , admin_forecast_wsizes_path,:class=> "list-group-item" %>

          </div>
        </div><!--/span-->
        <div class="col-xs-12 col-sm-10">
          <p class="pull-right visible-xs">
            <button type="button" class="btn btn-primary btn-xs" data-toggle="offcanvas">Toggle nav</button>
          </p>
          <div class="row">
           <%= yield %>            
          </div><!--/row-->
        </div><!--/span-->

      </div><!--/row-->

      <hr>

      <footer>
        <p>&copy; 99weiyi 2013</p>
      </footer>

    </div><!--/.container-->



    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="offcanvas.js"></script>
  </body>
</html>
