<%#= link_to '新型号', new_admin_model_path %>
<h1>问题列表</h1>

<table class="table table-hover table-condensed">
  <tr>
    <th>提问者</th>
    <th>服装链接</th>
    <th>上衣类型</th>
    <th>下衣类型</th>
    <th>内容</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @questions.each do |question| %>
  <tr>
    <td><%= question.user.try(:login) %></td>
    <td><%= question.url %></td>
    <td><%= question.up_size_id %></td>
    <td><%= question.down_size_id %></td>
    <td><%= question.content %></td>
    <td><%= link_to '显示', [:admin,question] %></td>
    <td><%= link_to '编辑', edit_admin_model_path(question) %></td>
    <td><%= link_to '删除', [:admin,question], confirm: '确定删除?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />


