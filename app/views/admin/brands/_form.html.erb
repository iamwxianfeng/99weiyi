<%= form_for([:admin,@brand]) do |f| %>
  <%= error_messages_for @brand %>

  <div class="field">
    <%= f.label "品牌名字" %><br />
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label "服装类型" %> <br/>
    <% Type.find_each do |t|  %>
      <%= check_box_tag "types[]", t.id, (true if @type_ids.index(t.id))  %> <%= t.name %> <br/>
    <% end %>
  </div>
  <div class="actions">
    <%= f.submit "提交" %>
  </div>
<% end %>
